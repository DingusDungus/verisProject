<%- include("admin-header", {title: title}); %>
<br>
<h3>Status of item</h3>
<div style="overflow-x:auto;">
    <table class="table">
        <% if (results[1].length > 0) { %>
        <thead class="thead-dark">
        <tr>
            <th scope="col">Booking Id</th>
            <th scope="col">Username</th>
            <th scope="col">Email</th>
            <th scope="col">Booked</th>
        </tr>
      </thead>
            <tr>
          <td scope="row"><%= results[1][0].booking_id %></td>
          <td><%= results[1][0].username %></td>
          <td><%= results[1][0].email %></td>
          <td><%= results[1][0].booked.toLocaleDateString(); %></td>
        </tr>
        <% } %>
    </table>
    </div>
    <div style="overflow-x:auto;">
      <table class="table">
          <% if (results[0].length > 0) { %>
          <thead class="thead-dark">
          <tr>
            <th scope="col">Booking Id</th>
            <th scope="col">Username</th>
            <th scope="col">Reserved</th>
          </tr>
        </thead>
              <tr>
            <td scope="row"><%= results[0][0].booking_id %></td>
            <td><%= results[0][0].username %></td>
            <td><%= results[0][0].reserved.toLocaleDateString(); %></td>
          </tr>
          <% } %>
      </table>
      </div>
      <% if (results[0].length == 0 && results[1].length == 0) { %>
        <p>Item is currently free</p>
      <% } %>
<%- include("footer"); %>